<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

{{# if condition}}
    condition is met
{{ else }}
    condition is not met
{{/ if}}

<ul>
{{# each anyArray}}
    <li>{{ this }}</li>
{{/each}}
</ul>

<ul>
{{# each anyArray as |val key|}}
    <li>{{key}}: {{ val }}</li>
{{/each}}
</ul>


<form action="/test/submit" method="POST">
<input type="text" name="id">
<button type="submit">submit</button>


</form>